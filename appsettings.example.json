{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=LMSDB;Username=your_username;Password=your_password;Include Error Detail=true"
  },
  "Jwt": {
    "Key": "your_jwt_secret_key_min_32_chars_long",
    "Issuer": "https://localhost:7044",
    "Audience": "https://localhost:4001",
    "ExpiryInDays": 7
  },
  "Email": {
    "SmtpHost": "smtp.gmail.com",
    "SmtpPort": 587,
    "Username": "your_email@gmail.com",
    "Password": "your_app_specific_password",
    "FromEmail": "your_email@gmail.com",
    "FromName": "Library Book Store"
  },
  "Cloudinary": {
    "CloudName": "your_cloud_name",
    "ApiKey": "your_api_key",
    "ApiSecret": "your_api_secret"
  }
}